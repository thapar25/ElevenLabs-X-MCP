   "source": [
    "import base64\n",
    "\n",
    "\n",
    "def file_to_base64(filepath):\n",
    "    with open(filepath, \"rb\") as f:\n",
    "        encoded = base64.b64encode(f.read()).decode(\"utf-8\")\n",
    "    return encoded"
   ]
  },
